<h1>Listing Events</h1>
<h2>This is a rails view (.erb)</h2>

 
<table class="table table-striped">
  <tr>
    <th>Event Title</th>
    <th>Event City</th>
    <th>Seatgeek ID</th>
    <th>Eventful ID</th>
    <th>Ticket URL</th>
    <th>FlipTix <i class="fa fa-ticket" aria-hidden="true"></i> Available?</th>
    <th>Venue</th>
  </tr>
 
  <% @events.each do |event| %>
    <tr>
      <td><%= event.event_title %></td>
      <td><%= event.event_city %></td>
      <td><%= event.seatgeek_eventid %></td>
      <td><%= event.eventful_eventid %></td>
      <td><i class="fa fa-ticket" aria-hidden="true"></i></td>
      <td><i class="fa fa-ticket" aria-hidden="true"></i></td>
      <td><a href="venues/<%= event.venue_id %>">Venue</a></td> 
    </tr>
  <% end %>
</table>

