<h1>Listing Venues</h1>
<h2>This is a rails view (.erb)</h2>
 
<table class="table table-striped">
  <tr>
    <th>Venue</th>
    <th>Venue City</th>
    <th>Venue Source</th>
    <th>Venue Source ID</th>

  </tr>
 
  <% @venues.each do |venue| %>
    <tr>
      <td><%= venue.name %></td>
      <td><%= venue.city %></td>
      <td><%= venue.venue_source %></td>
      <td><%= venue.venue_sourceid %></td>

    </tr>
  <% end %>
</table>

