<h1>Listing Venues</h1>
<h2>This is a rails view (.erb)</h2>
 
<table class="table table-striped">
  <tr>
    <th>Venue</th>
    <th>Venue City</th>
    <th>Seatgeek Venue ID</th>
    <th>Eventful Venue ID</th>

  </tr>
 
  <% @venues.each do |venue| %>
    <tr>
      <td><%= venue.name %></td>
      <td><%= venue.city %></td>
      <td><%= venue.seatgeek_venueid %></td>
      <td><%= venue.eventful_venueid %></td>

    </tr>
  <% end %>
</table>

